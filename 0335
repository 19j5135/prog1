for x in range(1,10):
    s=print()
    print(x*1, end=" ")
    s
    print(x*2, end=" ")
    s
    print(x*3, end=" ")
    s
    print(x*4, end=" ")
    s
    print(x*5, end=" ")
    s
    print(x*6, end=" ")
    s
    print(x*7, end=" ")
    s
    print(x*8, end=" ")
    s
    print(x*9, end=" ")
